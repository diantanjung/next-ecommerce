{
  "extends": ["eslint:recommended",  "next/core-web-vitals"],
  "rules": 
  {
    "no-unused-vars": 
    ["error", 
      { 
        "args": "all",
        "argsIgnorePattern": "^_",
        "caughtErrors": "all",
        "caughtErrorsIgnorePattern": "^_",
        "destructuredArrayIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "ignoreRestSiblings": true 
      }
    ]
  },
  "env": {
    "es2020": true
  }
}
